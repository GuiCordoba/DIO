    {
      "cell_type": "code",
      "source": [
        "# helper function to load image and return it and input vector\n",
        "def get_image(path):\n",
        "    img = tf.keras.utils.load_img(path, target_size=(224, 224))\n",
        "    x = tf.keras.utils.img_to_array(img)\n",
        "    x = np.expand_dims(x, axis=0)\n",
        "    x = preprocess_input(x)\n",
        "    return img, x"
      ],
      "metadata": {
        "id": "FGvGV55zgzkR"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Removendo arquivos Thumbs.db do database\n",
        "os.remove('/content/drive/MyDrive/DIO/Dogs_vs_Cat/data/PetImages/Cat/Thumbs.db')\n",
        "os.remove('/content/drive/MyDrive/DIO/Dogs_vs_Cat/data/PetImages/Dog/Thumbs.db')"
      ],
      "metadata": {
        "id": "MFseju9Lg4VG"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# É necessário uma redução do número de imagens para não ultrapassar o limite de RAM do google collab.\n",
        "for i in range(201, 12500):\n",
        "  os.remove(f'/content/drive/MyDrive/DIO/Dogs_vs_Cat/data/PetImages/Cat/{i}.jpg')\n",
        "  os.remove(f'/content/drive/MyDrive/DIO/Dogs_vs_Cat/data/PetImages/Dog/{i}.jpg')"
      ],
      "metadata": {
        "id": "9hlV4YiopHtm"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "data = []\n",
        "for c, category in enumerate(categories):\n",
        "    images = [os.path.join(dp, f) for dp, dn, filenames \n",
        "              in os.walk(category) for f in filenames \n",
        "              if os.path.splitext(f)[1].lower() in ['.jpg','.png','.jpeg']]\n",
        "    for img_path in images:\n",
        "        img, x = get_image(img_path)\n",
        "        data.append({'x':np.array(x[0]), 'y':c})\n",
        "\n",
        "# count the number of classes\n",
        "num_classes = len(categories)"
      ],
      "metadata": {
        "id": "AxVX4wDL6a1a"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "random.shuffle(data)"
      ],
